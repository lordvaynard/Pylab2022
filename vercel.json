{
    "builds": [{
        "src": "seletive/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "seletive/wsgi.py"
        }
    ],
    "env": {
        "SECRET_KEY": "@SECRET_KEY"
      }
}